    "        self.text_box.insert(\"end\", response + \"\\n***\\n\")\n",
    "\n",
    "        translated_text1 = translate_english_to_malayalam(response)\n",
    "        self.text_box.insert(\"end\", translated_text1 + \"\\n***\")\n",
    "\n",
    "        self.start_button.config(state=\"normal\")\n",
    "\n",
    "\n",
    "if __name__ == \"__main__\":\n",
